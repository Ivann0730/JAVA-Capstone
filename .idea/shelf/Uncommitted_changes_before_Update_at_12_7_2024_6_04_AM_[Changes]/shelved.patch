+        attackUp2 = setUp("/monster/orc_attack_up_2",gp.tileSize*2,gp.tileSize*2);
+        attackDown1 = setUp("/monster/orc_attack_down_1",gp.tileSize*2,gp.tileSize*2);
+        attackDown2 = setUp("/monster/orc_attack_down_2",gp.tileSize*2,gp.tileSize*2);
+        attackLeft1 = setUp("/monster/orc_attack_left_1",gp.tileSize*2,gp.tileSize*2);
+        attackLeft2 = setUp("/monster/orc_attack_left_2",gp.tileSize*2,gp.tileSize*2);
+        attackRight1 = setUp("/monster/orc_attack_right_1",gp.tileSize*2,gp.tileSize*2);
+        attackRight2 = setUp("/monster/orc_attack_right_2",gp.tileSize*2,gp.tileSize*2);
+    }
